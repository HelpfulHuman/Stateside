{
  "name": "stateside",
  "version": "1.2.1",
  "description": "Super simplified router implementation for React.",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push origin master && git push origin master --tags",
    "clean": "rm -rf dist/",
    "build": "rollup -c",
    "watch": "npm run build -- --watch",
    "test": "mocha",
    "tdd": "npm run watch | npm run test -- --watch",
    "prepublish": "npm run clean && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpfulhuman/stateside.git"
  },
  "keywords": [
    "react",
    "router",
    "route",
    "history",
    "location",
    "small",
    "tiny"
  ],
  "author": "Nick Glenn <nick@helpfulhuman.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helpfulhuman/stateside/issues"
  },
  "homepage": "https://github.com/helpfulhuman/stateside#readme",
  "dependencies": {
    "history": "^4.7.2",
    "object.omit": "^3.0.0",
    "path-to-regexp": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.3.1"
  }
}
