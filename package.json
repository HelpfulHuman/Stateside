{
  "name": "stateside",
  "version": "1.4.0",
  "description": "Super simplified router implementation for React.",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push origin master && git push origin master --tags",
    "clean": "rm -rf dist/",
    "build": "rollup -c",
    "watch": "npm run build -- --watch",
    "test": "mocha",
    "tdd": "npm run watch | npm run test -- --watch",
    "prepublish": "npm run clean && npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpfulhuman/stateside.git"
  },
  "keywords": [
    "react",
    "router",
    "route",
    "history",
    "location",
    "small",
    "tiny"
  ],
  "author": "Nick Glenn <nick@helpfulhuman.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helpfulhuman/stateside/issues"
  },
  "homepage": "https://github.com/helpfulhuman/stateside#readme",
  "dependencies": {
    "history": "4.7.2",
    "path-to-regexp": "2.0.0",
    "prop-types": "15.6.0",
    "react": "16.1.1"
  },
  "devDependencies": {
    "@types/history": "4.6.2",
    "@types/path-to-regexp": "1.7.0",
    "@types/react": "16.0.24",
    "chai": "4.1.2",
    "mocha": "3.5.3",
    "rollup": "0.50.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-typescript2": "0.8.0",
    "rollup-watch": "4.3.1",
    "typescript": "2.6.1"
  }
}
